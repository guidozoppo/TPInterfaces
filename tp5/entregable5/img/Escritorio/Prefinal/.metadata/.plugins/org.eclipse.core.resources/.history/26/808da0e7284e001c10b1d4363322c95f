import java.util.ArrayList;
import java.util.Comparator;

public class GrupoSponsoreado extends Grupo {

	private Video video;
	
	public GrupoSponsoreado(int demoraCarga, Video video) {
		super(demoraCarga);
		this.video = video;
	}
	
	@Override
	public ArrayList<Video> buscar(Filtro f, Comparator<Video> c) {
		ArrayList<Video> aRetornar = new ArrayList<>();
		if(video != null) {
			aRetornar.add(video);
		}
		for(VideoAbstracto elem: this.getElementos()) {
			aRetornar.addAll(elem.buscar(f, c));
		}
		return aRetornar;
	}

	public Video getVideo() {
		return video;
	}
	public void setVideo(Video video) {
		this.video = video;
	}

	


}
